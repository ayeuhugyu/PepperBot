    "description": "various testing and experiments of stuff",
    "main": "index.js",
    "scripts": {
        "start": "bun run src/index.ts",
        "gpt": "bun run tests/gpt_tester.ts",
        "pull": "git pull origin rewrite && (git diff --name-only HEAD@{1} HEAD | grep -q 'package.json' && bun install || true)",
        "migrate": "bunx knex migrate:latest",
        "migrate:make": "bunx knex migrate:make",
        "setup": "cp .env.example .env && bun install && bun run migrate",
        "start:server": "bun run pull && bun run migrate && bun run start"
    },
